<div class="page-header">
    <h1><%= @user.name %> <%= @user.last_name %> <small> profile</small></h1>
</div>
<ul class="breadcrumb">
    <li><%= link_to 'Home', '/' %> <span class="divider">&raquo;</span></li>
    <li><%= link_to 'Users', users_path %> <span class="divider">&raquo;</span></li>
    <li class="active"><%= @user.name %> <%= @user.last_name %></li>
</ul>

<p id="notice"><%= notice %></p>

<p>
<%= link_to "Back", users_path, :class=>'btn' %>
<%= link_to 'Edit', edit_user_path(@user), :class=>'btn' %> 
</p>

<p>
  <b>Email:</b>
  <%= @user.email %>
</p>

<p>
  <b>Password:</b>
  <%= @user.password %>
</p>

<p>
  <b>Name:</b>
  <%= @user.name %>
</p>

<p>
  <b>Last name:</b>
  <%= @user.last_name %>
</p>

<p>
  <b>Status:</b>
  <%= @user.status %>
</p>

<p>
  <b>Token:</b>
  <%= @user.token %>
</p>
